<h1>一覧</h1>
<hr>
<h3>物件一覧</h3>
<table>
  <tr>
    <th>物件名</th>
    <th>賃料</th>
    <th>築年数</th>
    <th>住所</th>
    <th>備考</th>
  </tr>
  <% @items.each do |item| %>
  <tr>
    <td><%= item.name %></td>
    <td><%= item.price %></td>
    <td><%= item.age %></td>
    <td><%= item.address %></td>
    <td><%= item.remark %></td>
  </tr>
  <% end %>
</table>
